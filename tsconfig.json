{
  "compilerOptions": {
	"target": "ES2024",
	"module": "ESNext",
	"moduleResolution": "Node",
	"jsx": "react-jsx",
	"declaration": true,
	"declarationDir": "dist",
	"emitDeclarationOnly": false,
	"outDir": "dist",
	"strict": true,
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"skipLibCheck": true
  },
  "include": ["../Mui-Simple-Dropdown/src"],
  "exclude": ["node_modules", "dist"]
}